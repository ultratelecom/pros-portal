# Supabase Configuration
SUPABASE_URL="https://your-project-ref.supabase.co"
SUPABASE_ANON_KEY="your-anon-key-here"

# Database URL for Prisma (PostgreSQL)
# Format: postgresql://postgres.[PROJECT_REF]:[PASSWORD]@db.[PROJECT_REF].supabase.co:5432/postgres
DATABASE_URL="postgresql://postgres.your-project-ref:your-password@db.your-project-ref.supabase.co:5432/postgres"

# JWT Secret for authentication (generate a secure random string)
JWT_SECRET="your-secure-jwt-secret-key-change-in-production"

# Node Environment
NODE_ENV="development" 